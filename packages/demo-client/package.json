  "devDependencies": {
    "@gen_epix/eslint-config": "*",
    "@gen_epix/typescript-config": "*",
    "@types/react": "19.1.4",
    "@types/react-dom": "19.1.5",
    "@vitejs/plugin-react-swc": "3.9.0",
    "eslint": "9.26.0",
    "typescript": "5.8.3",
